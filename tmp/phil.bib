@Article{BledaShackley:FormationRiskPerc,
   title =	{Simulation Modelling as a Theory Building Tool: The Formation of Risk Perceptions},
   author =	{Bleda, Mercedes and Shackley, Simon},
   journal =	{Journal of Artificial Societies and Social Simulation},
   ISSN =	{1460-7425},
   volume =	{15},
