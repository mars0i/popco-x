@Article{GrossmanEtAl:CompositeMultipleSignals,
  author =      {Grossman, Sharon R. and Shylakhter, Ilya and Karlsson, 
		Elinor K. and Byrne, Elizabeth H. and Morales, Shannon and 
		Frieden, Gabriel and Hostetter, Elizabeth and Angelino, Elaine
		and Garber, Manuel and Zuk, Or and Lander, Eric S. and 
		Schaffner, Stephen F. and Sabeti, Pardis C.},
  title =       {A Composite of Multiple Signals Distinguishes Causal 
